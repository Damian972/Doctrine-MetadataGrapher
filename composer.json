{
    "name":                 "onurb/doctrine-metadata-grapher",
    "description":          "Helper to format doctrine metadata for UML API",
    "type":                 "library",
    "licence":              "MIT",
    "keywords":             [
        "doctrine",
        "orm",
        "yuml",
        "metadata",
        "grapher",
        "diagram"
    ],
    "homepage":             "http://www.doctrine-project.org/",
    "authors":              [
      {
        "name":         "Bruno Heron",
        "email":        "herobrun@gmail.com"
      },
      {
            "name":         "Marco Pivetta",
            "email":        "ocramius@gmail.com"
        }
    ],
    "require":              {
        "php":              ">=5.3.2",
        "doctrine/common":  ">=2.5"
    },
    "require-dev":          {
        "phpunit/phpunit":  "4.4.0"
    },
    "suggest":              {
        "onurb/doctrine-yuml-bundle":            "if you use symfony, you will like this little tool"
    },
    "autoload":             {
        "psr-4":    { "Doctrine\\ORM\\MetadataGrapher\\":"lib/Doctrine/ORM/MetadataGrapher/" }
    },

}
